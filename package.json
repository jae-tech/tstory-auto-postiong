{
  "name": "tstory",
  "version": "1.0.0",
  "description": "OCI A1 기반 알뜰폰 자동 포스팅 시스템",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build -b swc --type-check",
    "build:tsc": "nest build",
    "build:webpack": "nest build --webpack",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "start:prod": "node dist/main",
    "dev": "pnpm run prisma:generate && nest build --webpack --watch",
    "dev:swc": "pnpm run prisma:generate && nest start -b swc --watch",
    "dev:tsc": "pnpm run prisma:generate && nest start --watch",
    "debug": "pnpm run prisma:generate && nest start -b swc --debug --watch",
    "debug:tsc": "pnpm run prisma:generate && nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:e2e": "vitest run --config ./vitest.e2e.config.ts",
    "test:ui": "vitest --ui",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:seed": "pnpm run prisma:seed",
    "db:reset": "pnpm run prisma:migrate:reset",
    "db:setup": "pnpm run prisma:generate && pnpm run prisma:migrate:dev",
    "docker:db": "docker-compose up -d postgres",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f postgres",
    "docker:clean": "docker-compose down -v"
  },
  "dependencies": {
    "@google/genai": "^1.24.0",
    "@google/generative-ai": "^0.24.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-fastify": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@prisma/client": "^6.17.1",
    "playwright": "^1.56.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.6",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@types/node": "^24.7.2",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "run-script-webpack-plugin": "^0.2.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "swc-loader": "^0.2.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.1.8",
    "webpack": "^5.102.1",
    "webpack-node-externals": "^3.0.0"
  }
}
